<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.Controller.LoginController">
   <children>
       <TextField fx:id="nombre" promptText="Ingrese su nombre" />
       <TextField fx:id="apellido" promptText="Ingrese su apellido" />
       <TextField fx:id="dni" promptText="Ingrese su dni" />
      <Button fx:id="DatosBoton" mnemonicParsing="false" text="Continuar" onAction="#handleLoginButtonAction" />
   </children>
</VBox>
